{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "target",

    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "target": "ES2020",

    "composite": true,
    "declaration": true,
    "experimentalDecorators": false,
    "importHelpers": true,
    "noImplicitOverride": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "dom.iterable",
      "es2020",
      "es2021.promise",
      "es2022.string",
      "es2022.array",
      "es2022.error",
      "es2024.promise"
    ],
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "inlineSources": true,
    "types": []
  },
  "include": []
}
